{
  "short_name": "pollr",
  "name": "pollr",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    },
    {
      "src": "pollr-logo-white.svg",
      "type": "image/svg+xml",
      "sizes": "any"
    }
  ],
  "color_scheme": "dark",
  "custom_message": "Please sign up to the metanet to use pollr. A decentralized, secure way to create and vote in polls.",
  "redirected_from": "https://pollr.gg",
  "start_url": ".",
  "Originator": "https://pollr.gg",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "babbage": {
    "groupPermissions": {
      "protocolPermissions": [
        {
          "protocolID":[1,"identity resolution"],
          "counterparty": "self",
          "description": "For viewing other's polls and voting"
        },
        {
          "protocolID":[2,"pollr"],
          "counterparty": "anyone",
          "description": "for voting, creating and closing polls"
        },
        {
          "protocolID":[2,"identity key retrieval"],
          "counterparty": "self",
          "description": "for identity key retrieval"
        }
      ],
      "basketAccess":[
        {
          "basket": "myPolls",
          "description": "For storing polls made by user."
        },
        {
          "basket": "myvotes",
          "description": "For storing votes made by user."
        }
      ]
    }
  }
}

